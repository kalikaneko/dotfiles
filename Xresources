/*urxvt.perl-lib:                      /home/kali/urxvt-font*/
urxvt.perl-lib:  		     /home/kali/urxvt-perls
urxvt.keysym.Control-Shift-F11:      perl:font:decrement
urxvt.keysym.Control-Shift-F12:      perl:font:increment
URxvt.perl-ext-common:		     default,font,tabbed,url-select
/* matcher */
/* url-select */
URxvt.keysym.M-u: 		     perl:url-select:select_next
URxvt.url-select.launcher: 	     chromium
URxvt.url-select.underline:          true

/* Tabbed is AWESOME!!! */
/* Can I has the tabs greenish??? */
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    3
URxvt.tabbed.tab-bg:    0

/* matcher: open links from keyboard */
/*
URxvt.keysym.C-Delete:  perl:matcher:last
URxvt.keysym.M-Delete:  perl:matcher:list
*/

/* color URLs */
URxvt.colorUL: #4682B4

